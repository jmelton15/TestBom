pipeline {
    agent any

    stages {
        stage('version') {
            steps {
                sh 'python3 --version'
            }
        }
        stage('RunScript_1') {
            steps {
                echo 'Running cdx server script....'
                sh 'python python2.py'
            }
        }
        stage('RunScript_2') {
            steps {
                echo 'Running SBOM Script....'
                sh 'python sbom_script.py'
            }
        }
    }
}